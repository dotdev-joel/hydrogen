{
  "name": "@shopify/cli-hydrogen",
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "version": "5.1.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup --config ./tsup.config.ts && node scripts/build-check.mjs",
    "dev": "tsup --watch --config ./tsup.config.ts",
    "typecheck": "tsc --noEmit",
    "generate:manifest": "oclif manifest",
    "test": "cross-env SHOPIFY_UNIT_TEST=1 vitest run --test-timeout=15000",
    "test:watch": "cross-env SHOPIFY_UNIT_TEST=1 vitest --test-timeout=15000"
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/prettier": "^2.7.2",
    "@types/recursive-readdir": "^2.2.1",
    "@types/tar-fs": "^2.0.1",
    "oclif": "3.9.1",
    "tempy": "^3.0.0",
    "type-fest": "^3.6.0",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "@remix-run/react": "^1.17.1",
    "@shopify/hydrogen-react": "^2023.7.0",
    "@shopify/remix-oxygen": "^1.1.1"
  },
  "dependencies": {
    "@ast-grep/napi": "^0.5.3",
    "@graphql-codegen/cli": "3.3.1",
    "@oclif/core": "2.8.5",
    "@remix-run/dev": "1.17.1",
    "@shopify/cli-kit": "3.47.5",
    "@shopify/hydrogen-codegen": "^0.0.2",
    "@shopify/mini-oxygen": "^1.7.0",
    "ansi-escapes": "^6.2.0",
    "diff": "^5.1.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "gunzip-maybe": "^1.4.2",
    "prettier": "^2.8.4",
    "recursive-readdir": "^2.2.3",
    "tar-fs": "^2.1.1",
    "typescript": "^4.9.5"
  },
  "bin": "dist/create-app.js",
  "exports": {
    "./package.json": "./package.json",
    "./commands/hydrogen/init": "./dist/commands/hydrogen/init.js"
  },
  "files": [
    "dist",
    "oclif.manifest.json"
  ],
  "engines": {
    "node": ">=16.13"
  },
  "oclif": {
    "commands": "dist/commands",
    "hooks": {
      "init": "./dist/hooks/init.js"
    },
    "topics": {
      "hydrogen": {
        "description": "Hydrogen commands"
      }
    }
  }
}
